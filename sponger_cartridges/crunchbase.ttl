# <-------------- Crunchbase --------------> #

# <-------------- Crunchbase Classes --------------> #

oplcb:Acquisition
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    rdfs:subClassOf olo:OrderedList ;
    rdfs:label "Acquisition" ;
    rdfs:comment "Acquisition"@en .

oplcb:FundingRound
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    rdfs:label "Funding round" ;
    rdfs:comment "Funding round"@en .

oplcb:Investment
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    rdfs:label "Investment" ;
    rdfs:comment "Investment"@en .

oplcb:Milestone
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    rdfs:label "Milestone" ;
    rdfs:comment "Milestone"@en .

oplcb:Office
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    rdfs:label "Office" ;
    rdfs:comment "Office"@en .

oplcb:Product
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    rdfs:label "Product" ;
    rdfs:comment "Product"@en .

oplcb:Providership
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    rdfs:label "Providership" ;
    rdfs:comment "Providership"@en .

oplcb:Relationship
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    rdfs:label "Relationship" ;
    rdfs:comment "Relationship"@en .

oplcb:Screenshot
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    rdfs:label "Screenshot" ;
    rdfs:comment "Screenshot"@en .

oplcb:VideoEmbed
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    rdfs:label "Video embed" ;
    rdfs:comment "Video embed"@en .

oplcb:Provider
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    rdfs:label "Provider" ;
    rdfs:comment """A legal, PR & marketing firms that provides services to this company"""@en .

oplcb:Investor
	a owl:Class , foaf:Agent;
	rdfs:label "Investor" ;
	rdfs:comment "Participant in an investment that provides capital to a company"@en ;
	is rdfs:range of oplcb:investor .

# <-------------- Crunchbase Properties --------------> #

oplcb:hasAcquiredDay
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    skos:altLabel "acquired day" ;
    rdfs:label "hasAcquiredDay" ;
    rdfs:comment "The day of an acquistion"@en ;
    rdfs:range xsd:integer;
    rdfs:domain oplcb:Acquisition .

oplcb:hasAcquiredMonth
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    skos:altLabel "acquired month" ;
    rdfs:label "hasAcquiredMonth" ;
    rdfs:comment "The month of an acquisition"@en ;
    rdfs:domain oplcb:Acquisition ;
    rdfs:range xsd:integer .

oplcb:hasAcquiredYear
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    skos:altLabel "acquired year" ;
    rdfs:label "hasAcquiredYear" ;
    rdfs:comment "The year of an acquistion"@en ;
    rdfs:range xsd:integer ;
    rdfs:domain oplcb:Acquisition .

oplcb:hasAcquiredCompany
    a owl:ObjectProperty , rdf:Property ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    skos:altLabel "acquiring company" ;
    rdfs:label "hasAcquiredCompany" ;
    rdfs:comment "The company acquiring this one"@en ;
    rdfs:domain oplcb:Acquisition ;
    rdfs:range foaf:Organization .

oplcb:hasAcquisition
    a owl:ObjectProperty , rdf:Property ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/crunchbase#> ;
    rdfs:label "hasAcquisition" ;
    rdfs:comment """Associates a Business Entity with its acquisitions collection"""@en ;
    rdfs:range oplcb:Acquisition;
    rdfs:domain foaf:Organization, gr:BusinessEntity .

oplcb:hasFundingRound
    a owl:ObjectProperty , rdf:Property ;
