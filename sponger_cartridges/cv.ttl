# EduDegree individuals
#
cv:EduAssociate
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/cv#> ;
    a cv:EduDegree;
    rdfs:label "Associate degree" .

cv:EduBachelor
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/cv#> ;
    a cv:EduDegree;
    rdfs:label "Bachelor's degree" .

cv:EduCollegeCoursework
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/cv#> ;
    a cv:EduDegree;
