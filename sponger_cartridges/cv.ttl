cv:EduDoctorate
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/cv#> ;
    a cv:EduDegree;
    rdfs:label "Doctorate" .

cv:EduHighSchool
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/cv#> ;
    a cv:EduDegree;
    rdfs:label "High school or equivalent." .

cv:EduMaster
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/cv#> ;
    a cv:EduDegree;
    rdfs:label "Master's degree" .

cv:EduProfessional
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/cv#> ;
    a cv:EduDegree;
