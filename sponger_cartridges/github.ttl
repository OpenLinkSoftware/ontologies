# < ------------------- Classes ------------------------>

gh:Comment
    a owl:Class ;
    rdfs:subClassOf schema:Comment ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/github#> ;
    rdfs:label "Comment" ;
    rdfs:comment """A class for comments"""@en .

gh:Gist
    a owl:Class ;
    rdfs:subClassOf foaf:Document ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/github#> ;
    rdfs:label "GitHub Gist" ;
    rdfs:comment """Gist is a simple way to share snippets and pastes with others. All gists are Git repositories, so they are automatically versioned, forkable and usable from Git."""@en .

gh:Pull
    a owl:Class ;
    rdfs:subClassOf foaf:Document ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/github#> ;
    rdfs:label "GitHub Pull" ;
    rdfs:comment """A class for describing pull requests"""@en .

gh:Task
    a owl:Class ;
    rdfs:subClassOf wf:Task ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/github#> ;
    rdfs:label "Task" ;
    rdfs:comment """A class for describing tasks"""@en .

# < ------------------- Properties ------------------------>

gh:additions
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/github#> ;
    rdfs:label "additions" ;
    skos:altLabel "additions" ;
    rdfs:comment """The number of additions associated with a pull request"""@en ;
    rdfs:domain gh:Pull ;
    rdfs:range xsd:integer .

gh:changedFiles
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/github#> ;
    rdfs:label "changedFiles" ;
    skos:altLabel "changedFiles" ;
    rdfs:comment """The number of changed files associated with a pull request"""@en ;
    rdfs:domain foaf:Agent ;
    rdfs:range xsd:integer .

