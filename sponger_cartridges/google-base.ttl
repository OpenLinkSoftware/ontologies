    a owl:DatatypeProperty , rdf:Property ;
    rdfs:subPropertyOf gr:datatypeProductOrServiceProperty ;
    rdfs:isDefinedBy <http://www.openlinksw.com/schemas/google-base#> ;
    rdfs:label "id" ;
    skos:altLabel "id" ;
    rdfs:comment """ID"""@en ;
    rdfs:domain oplgb:ItemType ;
