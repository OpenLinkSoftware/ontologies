# <-------------- Web Design --------------> #

# <-------------- Web Design Classes --------------> #

oplwebdesign:AdvertisingBlock
    a owl:Class ;
    rdfs:label "Advertising Block"^^xsd:string  ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:subClassOf oplwebdesign:WebmasterBlock .

oplwebdesign:Audio
    a owl:Class ;
    rdfs:comment "Audio element (music, speech, etc)"@en ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Audio element" ;
    rdfs:subClassOf oplwebdesign:MediaElement .

oplwebdesign:ColumnContentBlock
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Content-block for columnar display" ;
    rdfs:subClassOf oplwebdesign:ContentBlock .

oplwebdesign:ContentBlock
    a owl:Class ;
    rdfs:comment "Superclass for various types of content block"@en ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Generic content block" ;
    rdfs:subClassOf oplwebdesign:DesignElement .

oplwebdesign:DesignElement
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Web Design Element" ;
    rdfs:subClassOf oplwebdesign:WebThing .

oplwebdesign:DocumentationLink
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Block for links to documentation" ;
    rdfs:subClassOf oplwebdesign:Related .

oplwebdesign:Footer
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Page footer" ;
    rdfs:subClassOf oplwebdesign:PageDecoration .

oplwebdesign:Header
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Page header" ;
    rdfs:subClassOf oplwebdesign:PageDecoration .

oplwebdesign:HorizNavBar
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Horizontal navigation menu" ;
    rdfs:subClassOf oplwebdesign:Navbar .

oplwebdesign:Image
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Image" ;
    rdfs:subClassOf oplwebdesign:MediaElement .

oplwebdesign:Layout
    a owl:Class ;
    rdfs:label "Layout"^^xsd:string  ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:subClassOf oplwebdesign:WebThing .

oplwebdesign:MediaElement
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Media element (audio/visual)" ;
    rdfs:subClassOf oplwebdesign:DesignElement, [
        a owl:Class ;
        owl:unionOf (oplwebdesign:Audio
            oplwebdesign:Image
            oplwebdesign:Video
        )
    ] .

oplwebdesign:Menu
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Menu (generic)" ;
    rdfs:subClassOf oplwebdesign:DesignElement .

oplwebdesign:MenuItem
    a owl:Class ;
    rdfs:label "Menu Item"^^xsd:string  ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:subClassOf oplwebdesign:Navbar .

oplwebdesign:MultiScaleImage
    a owl:Class ;
    rdfs:comment "An image for display at multiple sizes e.g. thumbnail+popup"@en ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Multi-scale image" ;
    rdfs:subClassOf oplwebdesign:Image .

oplwebdesign:Navbar
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Navigation menu (generic)" ;
    rdfs:subClassOf oplwebdesign:Menu .

oplwebdesign:Page
    a owl:Class ;
    rdfs:label "Page"^^xsd:string  ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:subClassOf oplwebdesign:WebThing .

oplwebdesign:PageDecoration
    a owl:Class ;
    rdfs:comment "General decorative features in a page"@en ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Page Decoration" ;
    rdfs:subClassOf oplwebdesign:DesignElement, [
        a owl:Class ;
        owl:unionOf (oplwebdesign:Footer
            oplwebdesign:Header
            oplwebdesign:SocialSection
        )
    ] .

oplwebdesign:Related
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Related content block" ;
    rdfs:subClassOf oplwebdesign:ContentBlock .

oplwebdesign:Site
    a owl:Class ;
    rdfs:label "Site"^^xsd:string  ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:subClassOf oplwebdesign:WebThing .

oplwebdesign:SocialSection
    a owl:Class ;
    rdfs:comment "Section for social media interaction - like, share, follow in various domains"@en ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Social section" ;
    rdfs:subClassOf oplwebdesign:PageDecoration .

oplwebdesign:TrackingBlock
    a owl:Class ;
    rdfs:label "Tracking Block"^^xsd:string  ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:subClassOf oplwebdesign:WebmasterBlock .

oplwebdesign:VerticalNavBar
    a owl:Class ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Vertical navigation menu" ;
    rdfs:subClassOf oplwebdesign:Navbar .

oplwebdesign:Video
    a owl:Class ;
    rdfs:comment "A video element"@en ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "Video element" ;
    rdfs:subClassOf oplwebdesign:MediaElement .

oplwebdesign:WebThing
    a owl:Class ;
    rdfs:label "Web Thing"^^xsd:string  ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> .

oplwebdesign:WebmasterBlock
    a owl:Class ;
    rdfs:comment "A superclass for various webmaster-related classes such as advertising blocks, adsense and tracking details, etc"@en ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "webmaster block" ;
    rdfs:subClassOf oplwebdesign:WebThing, [
        a owl:Class ;
        owl:unionOf (oplwebdesign:AdvertisingBlock
            oplwebdesign:TrackingBlock
        )
    ] .


# <-------------- Web Design Properties --------------> #

oplwebdesign:isAutoplayEnabled
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "Whether a MediaElement (video or audio) should automatically play"@en ;
    rdfs:domain oplwebdesign:MediaElement ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "isAutoplayEnabled" ;
    rdfs:range xsd:boolean .

oplwebdesign:hasCaption
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "Caption or title"@en ;
    rdfs:domain oplwebdesign:DesignElement ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "hasCaption" ;
    rdfs:range xsd:string ;
    owl:equivalentProperty dc:title .

oplwebdesign:contains
    a owl:ObjectProperty , rdf:Property ;
    rdfs:comment "generic containment relations between any Web Thing subclasses"@en ;
    rdfs:domain oplwebdesign:WebThing ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "contains" ;
    rdfs:range oplwebdesign:WebThing ;
    owl:subPropertyOf sioc:container_of .

oplwebdesign:hasCSSRequirement
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "Path to a required CSS library or stylesheet"@en ;
    rdfs:domain oplwebdesign:DesignElement ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "hasCSSRequirement" ;
    rdfs:range rdfs:Resource .

oplwebdesign:hasDisplayHeight
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "height to display an image"@en ;
    rdfs:domain oplwebdesign:MediaElement ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "hasDisplayHeight" ;
    rdfs:range xsd:integer .

oplwebdesign:hasDisplayWidth
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "width to display an image"@en ;
    rdfs:domain oplwebdesign:MediaElement ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "hasDisplayWidth" ;
    rdfs:range xsd:integer .

oplwebdesign:hasContent
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "Mapping from a container to literal content"@en ;
    rdfs:domain oplwebdesign:DesignElement ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "hasContent" ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf atomowl:content .

oplwebdesign:hasHTMLFormatString
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "An HTML fragment with %s placeholders and parameters to implement a DesignElement"@en ;
    rdfs:domain oplwebdesign:DesignElement ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "hasHTMLFormatString" ;
    rdfs:range xsd:string .

oplwebdesign:hasHTMLFragment
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "HTML fragment required to implement a DesignElement"@en ;
    rdfs:domain oplwebdesign:DesignElement ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "hasHTMLFragment" ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf atomowl:content .

oplwebdesign:hasReferenceImage
    a owl:ObjectProperty , rdf:Property ;
    rdfs:comment "Link a DesignElement to an Image it uses or references"@en ;
    rdfs:domain oplwebdesign:DesignElement ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "hasReferenceImage" ;
    rdfs:range oplwebdesign:Image ;
    owl:equivalentProperty foaf:depiction .

oplwebdesign:hasImageFile
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "An image used as part of a DesignElement (by filename)"@en ;
    rdfs:domain oplwebdesign:Image ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "hasImageFile" ;
    rdfs:range xsd:string ;
    owl:equivalentProperty foaf:depiction .

oplwebdesign:hasJavaScriptRequirement
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "Name of a required Javascript library (filename)"@en ;
    rdfs:domain oplwebdesign:DesignElement ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "hasJavaScriptRequirement" ;
    rdfs:range xsd:string .

oplwebdesign:hasLayout
    a owl:ObjectProperty , rdf:Property ;
    rdfs:comment "Mapping between a Site or Page and a Layout"@en ;
    rdfs:domain oplwebdesign:WebThing ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "hasLayout" ;
    rdfs:range oplwebdesign:Layout .

oplwebdesign:isLinkTo
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "destination of a MenuItem link"@en ;
    rdfs:domain oplwebdesign:MenuItem ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "isLinkTo" ;
    rdfs:range rdfs:Resource ;
    owl:equivalentProperty sioc:links_to .

oplwebdesign:hasMediaURL
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "Link from MediaElements to the actual media file used"@en ;
    rdfs:domain oplwebdesign:MediaElement ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "hasMediaURL" ;
    rdfs:range rdfs:Resource .

oplwebdesign:hasProvider
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "Name of a provider for a WebmasterBlock (e.g. Google)"@en ;
    rdfs:domain oplwebdesign:WebmasterBlock ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "hasProvider" ;
    rdfs:range xsd:string .

oplwebdesign:hasServiceIdentifier
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "Identifier in an upstream service (e.g. site code for analytics)"@en ;
    rdfs:domain oplwebdesign:WebmasterBlock ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "hasServiceIdentifier" ;
    rdfs:range xsd:string .

oplwebdesign:hasSocialPurpose
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "Purpose of a SocialSection block - follow, share, contact"@en ;
    rdfs:domain oplwebdesign:SocialSection ;
    rdfs:isDefinedBy <http://www.openlinksw.com/ontology/oplwebdesign#> ;
    rdfs:label "hasSocialPurpose" ;
    rdfs:range xsd:string .

oplwebdesign:hasThumbnailFile
    a owl:DatatypeProperty , rdf:Property ;
    rdfs:comment "An image used as part of a DesignElement (by filename)"@en ;
